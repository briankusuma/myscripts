                    </button>
                    <input
                    type="search"
                    name="q"
                    placeholder="Search"
                    value="{{ search.terms | escape }}"
                    aria-label="Search"
                    >
                </form>
            </div>

            <div class="navbar_menus-right">
                {% comment %} <a href="">Contact</a> {% endcomment %}
                {% comment %} <a href="/cart">
                    <img class="cart-icon" src="{{ 'cart-ic.svg' | asset_url }}" alt="Icon" width="24" height="24">
                </a> {% endcomment %}
                {% comment %} <a href="/account/login" class="button btn-outline is-small">Sign in</a> {% endcomment %}
                    


