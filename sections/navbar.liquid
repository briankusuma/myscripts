<div class="container">
    <div class="navbar_layout">
        <div class="navbar_button">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                <path d="M3.75 9H20.25M3.75 15.75H20.25" stroke="#274250" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
        </div>
        <div class="navbar_brand">
            <a href="/">
                {% if section.settings.logo_brand %}
                    <img class="logo-brand" src="{{ section.settings.logo_brand | image_url: width: 150 }}" alt="Logo Brand" width="150" height="30">
                {% else %}
                    <img class="logo-brand" src="{{ 'Logo-myscripts.svg' | asset_url }}" alt="Logo Brand" width="150" height="30">
                {% endif %}
            </a>
        </div>
        <div class="navbar_menus">
            <ul class="navbar_menus-left">
            {% for link in linklists.main-menu.links %}
                <li>
                    <a href="{{ link.url }}">
                        {{ link.title }}
                    </a>
                </li>
            {% endfor %}
            </ul>

            <div class="navbar-search">
                <form action="{{ routes.search_url }}" method="get" role="search">
                    <button type="submit">
