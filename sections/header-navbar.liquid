            <path d="M6.75 6.89167V6.08334C6.75 4.20834 8.25833 2.36667 10.1333 2.19167C12.3667 1.97501 14.25 3.73334 14.25 5.92501V7.07501" stroke="currentColor" stroke-width="1.25" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>
