            <path d="M13.4131 10.4999H13.4206" stroke="currentColor" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M7.57908 10.4999H7.58657" stroke="currentColor" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>
          </a>
        </li>
      </ul>
    </div>
